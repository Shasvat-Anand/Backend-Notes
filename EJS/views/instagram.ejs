  <h2>Posts</h2>
  <div class="posting">
    <% for (let post of data.posts) { %>
      <div class="postsdiv">
        <img class="img" src="<%= post.image %>" alt="post">
        <div class="info">
          <h3>‚ù§Ô∏è <%= post.likes %> Likes</h3>
          <h3>üí¨ <%= post.comments %></h3>
        </div>
      </div>
    <% } %>
  </div>

  <%- include("includes/footer.ejs")%>
</body>
 